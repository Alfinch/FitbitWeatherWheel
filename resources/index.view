    <g transform="rotate(330)"><line /></g>
    <g transform="rotate(345)"><line /></g>
  </g>
  
  <!-- 48 arcs making up 24 bars - 1 per hour -->
  <g id="rainBarsA">
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
  </g>
  
  <g id="rainBarsB">
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
    <arc /><arc /><arc /><arc />
  </g>
  
  <mask id="graphLineMask">
    <g id="graphLineTransform" transform="translate(168, 168)">
      <!-- 192 segments - 8 per hour -->
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
      <line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line /><line />
    </g>
  </mask>
    
  <svg mask="#graphLineMask" width="336" height="336">
    <gradientRect id="graphGradient" />
  </svg>
  
  <g id="dayHandTransform" transform="translate(168, 168)">
    <line id="dayHand" x1="0" y1="105" x2="0" y2="165" />
  </g>
  
  <g id="dataHandTransform" transform="translate(168, 168)">
    <line id="dataHand" x1="0" y1="105" x2="0" y2="165" />
  </g>
  
