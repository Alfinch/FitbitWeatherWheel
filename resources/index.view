  </g>
  <g id="loTempTransform">
    <text id="loTemp" />
  </g>
  
  <text id="temp" />
  <text id="time" />
  <text id="weather" />
  
  <g id="graphHourMarkers" transform="translate(168, 168)">
    <g transform="rotate(0)"><line /></g>
    <g transform="rotate(15)"><line /></g>
    <g transform="rotate(30)"><line /></g>
    <g transform="rotate(45)"><line /></g>
